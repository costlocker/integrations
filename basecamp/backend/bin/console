#!/usr/bin/env php
<?php

$app = require __DIR__ . '/../app/app.php';

$console = $app['console'];
$console->addCommands([
    new Costlocker\Integrations\Sync\Queue\ProcessSyncRequestsCommand(
        new Costlocker\Integrations\Sync\Queue\ProcessSyncRequest($app),
        $app['logger']
    ),
]);
$console->run();
