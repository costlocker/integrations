{
    "name": "costlocker/basecamp-sync",
    "autoload": {
        "psr-4": {
            "Costlocker\\Integrations\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/GivenApi.php",
            "tests/Basecamp/ApiCall.php",
            "tests/Basecamp/GivenBasecampConnect.php",
            "vendor/hamcrest/hamcrest-php/hamcrest/Hamcrest.php"
        ]
    },
    "config": {
        "optimize-autoloader": true
    },
    "require": {
        "vlucas/phpdotenv": "^2.4",
        "silex/silex": "^2.0",
        "monolog/monolog": "^1.22",
        "sentry/sentry": "^1.6",
        "guzzlehttp/guzzle": "^6.2",
        "league/oauth2-client": "^2.2",
        "14four/oauth2-basecamp": "^1.0",
        "symfony/console": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5",
        "mockery/mockery": "1.0.0-alpha1",
        "hamcrest/hamcrest-php": "^2.0",
        "symfony/browser-kit": "^3.2"
    }
}
